# vim:set ft=sh:

[ -f /usr/local/etc/bash_completion ] && . /usr/local/etc/bash_completion
[ -f ~/.aliases_mac ] && . ~/.aliases_mac

# If we have a `default` python virtualenv, lets use it over anything on the system already
[ -d ~/.virtualenvs/default ] && export PATH="$HOME/.virtualenvs/default/bin:$PATH"

export PATH="$PATH:/usr/local/sbin"

# homebrew installed vcprompt is from https://bitbucket.org/gward/vcprompt
export VCPROMPT_FORMAT="%b%u%m"

alias vlc="/Applications/VLC.app/Contents/MacOS/VLC"

[ -f '/Users/philipforget/workspace/google-cloud-sdk/path.bash.inc' ] && . '/Users/philipforget/workspace/google-cloud-sdk/path.bash.inc'
[ -f '/Users/philipforget/workspace/google-cloud-sdk/completion.bash.inc' ] && . '/Users/philipforget/workspace/google-cloud-sdk/completion.bash.inc'
